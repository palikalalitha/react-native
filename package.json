{
  "name": "StarterKit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint --ext .ts,.tsx app/"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@react-native-community/datetimepicker": "^2.6.0",
    "@types/i18n-js": "^3.0.1",
    "@types/micromatch": "^3.1.0",
    "@types/styled-components": "^4.1.19",
